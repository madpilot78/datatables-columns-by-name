{
    "name": "madpilot78/datatables-columns-by-name",
    "description": "Extract column data by name from datatables query string",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Guido Falsi",
            "email": "mad@madpilot.net"
        }
    ],
    "minimum-stability": "dev",
